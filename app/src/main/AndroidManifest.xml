<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ignacio.lorenz.prrcmobile">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/prrclogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity

            android:name="com.ignacio.lorenz.prrcmobile.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.ignacio.lorenz.prrcmobile.HomeActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="com.ignacio.lorenz.prrcmobile.Login" android:screenOrientation="portrait"></activity>
    </application>

</manifest>